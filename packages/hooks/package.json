{
  "name": "@dolthub/react-hooks",
  "author": "DoltHub",
  "description": "A collection of React hooks for common tasks",
  "version": "0.1.8",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "hooks",
    "react-hooks",
    "state",
    "frontend"
  ],
  "packageManager": "yarn@4.0.2",
  "scripts": {
    "compile": "tsc -b",
    "build": "rollup -c --bundleConfigAsCjs",
    "dbuild": "yarn compile && yarn build",
    "lint": "eslint --cache --ext .ts,.js,.tsx,.jsx src",
    "prettier": "prettier --check 'src/**/*.{js,ts}'",
    "prettier-fix": "prettier --write 'src/**/*.{js,ts}'",
    "npm:publish": "yarn dbuild && npm publish",
    "test": "jest --env=jest-environment-jsdom",
    "yalc:publish": "yarn dbuild && yalc publish",
    "yalc:push": "yarn dbuild && yalc push"
  },
  "peerDependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "dependencies": {
    "@dolthub/web-utils": "^0.1.5",
    "js-cookie": "^3.0.5",
    "react-hotkeys": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.7",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^11.1.5",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/babel__core": "^7",
    "@types/babel__preset-env": "^7",
    "@types/eslint": "^8",
    "@types/jest": "^29.5.14",
    "@types/js-cookie": "^3",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/rollup-plugin-peer-deps-external": "^2",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.16.0",
    "babel-jest": "^30.0.2",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.2",
    "prettier": "^3.6.2",
    "raf-stub": "^3.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rollup": "^4.46.2",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "yalc": "^1.0.0-pre.53"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dolthub/react-library.git"
  },
  "bugs": {
    "url": "https://github.com/dolthub/react-library/issues"
  }
}
